            spacing="true"
            expand="spacer">
        <label stylename="h1"
               value="Choose the new Target"
               width="100%"/>
        <comboBox id="cmbParent"
                  width="100%"/>
        <label id="spacer"/>
        <buttonsPanel>
            <button id="btnCancel" caption="Cancel" />
            <button id="btnConnect" caption="Connect" />
        </buttonsPanel>
    </layout>
</window>